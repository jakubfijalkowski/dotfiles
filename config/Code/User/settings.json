{
    "workbench.colorTheme": "Dracula",
    "workbench.iconTheme": "vscode-icons",
    "editor.fontFamily": "'Source Code Variable', 'Source Code Pro', Consolas, 'Courier New', monospace",
    "editor.rulers": [
        100
    ],
    "editor.lineNumbers": "relative",

    "explorer.openEditors.visible": 0,
    "explorer.autoReveal": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": false,
    "files.eol": "\n",

    "window.zoomLevel": 0,
    "window.menuBarVisibility": "toggle",

    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.blame.highlight.locations": [
        "overview",
        "gutter",
        "line"
    ],
    "gitlens.recentChanges.highlight.locations": [
        "gutter",
        "overview"
    ],
    "gitlens.codeLens.scopes": [
        "blocks",
        "containers"
    ],
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false, "suppressCommitNotFoundWarning": true, "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": false
    },
    "gitlens.strings.codeLens.unsavedChanges.recentChangeAndAuthors": "",
    "gitlens.strings.codeLens.unsavedChanges.recentChangeOnly": "",
    "gitlens.strings.codeLens.unsavedChanges.authorsOnly": "",
    "gitlens.keymap": "chorded",
    "gitlens.views.repositories.enabled": false,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,

    "typescript.check.npmIsInstalled": false,

    "vim.useSystemClipboard": true,
    "vim.foldfix": true,
    "vim.leader": ",",
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.normalModeKeyBindings": [
        {
            "before": [ "g", "r" ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.files.action.showActiveFileInExplorer",
                    "args": []
                }
            ]
        },
        {
            "after": [ "<leader>", "<leader>", "s" ],
            "before": [ "<leader>", "s" ]
        }
    ],
    "breadcrumbs.enabled": true,
    "msbuildProjectTools.nuget.includePreRelease": true,
    "msbuildProjectTools.nuget.newestVersionsFirst": true,

    "omnisharp.path": "latest",
    "omnisharp.enableEditorConfigSupport": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.useGlobalMono": "never",
    "csharp.maxProjectFileCountForDiagnosticAnalysis": 2000,
    "razor.disabled": true,

    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },

    "atlascode.jira.workingSite": {
        "baseUrlSuffix": "atlassian.net"
    },
    "atlascode.bitbucket.issues.explorerEnabled": false,
    "atlascode.bitbucket.pipelines.explorerEnabled": false,
    "atlascode.bitbucket.statusbar.enabled": false,
    "atlascode.jira.statusbar.showSite": true,
    "atlascode.jira.statusbar.showProject": true,
    "atlascode.jira.statusbar.showProduct": false,
    "atlascode.jira.statusbar.showUser": false,
    "atlascode.jira.statusbar.showLogin": false,
    "atlascode.showWelcomeOnInstall": false,
    "yaml.schemas": {
        "file:///home/fiolek/.vscode/extensions/atlassian.atlascode-2.2.1/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    }
}

